<div class="note">
  <div>
    <%= content_tag :span, "#{note.title} (hace #{time_ago_in_words note.created_at}).", class: "note-title" %>
    <% if  session[:reviewer_id] %>  
      <%= link_to "Delete Note", [@book, note], method: :delete %>
    <% end %>
  </div>
  <div><%= simple_format note.note %></div>
</div>
